{
	"name": "boiler",
	"packageManager": "yarn@3.5.0",
	"version": "1.4.0",
	"type": "module",
	"private": true,
	"license": "zlib-acknowledgement",
	"author": "chrono <chrono@disilla.org>",
	"main": "src/server.ts",
	"scripts": {
